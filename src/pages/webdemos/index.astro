---
import Layout from "src/layouts/Layout.astro"

const demos = [
    { id: 1, slug: '/webdemos/clock/', title: 'Animated Clock svg' },
    { id: 2, slug: '/webdemos/snake/', title: 'Snake game' },
    { id: 3, slug: '/webdemos/opl3/', title: 'OPL3 lib usage' },
    { id: 4, slug: '/webdemos/editorjs/', title: 'Editor.js demo page' },
    { id: 5, slug: '/webdemos/blocknote/', title: 'BlockNote.js demo page' },
];
---

<Layout title="Web demos and code examples" description="Random code snippets I'd like to test on the site; Testing ground for Astro.build and Svelte">
    <ul>
        {demos.map((e) =>
            <li><a href={e.slug}>{e.title}</a></li>
        )}
    </ul>
</Layout>
